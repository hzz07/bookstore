<!--排行榜-->
排行榜
<div class="mypaihang">
 <ul id="myTab" class="nav nav-tabs nav-justified myTab">
    <li class="active"><a href="#home" data-toggle="tab">畅销榜</a></li>
    <li><a href="#ios" data-toggle="tab">新书榜</a></li>
</ul>
<div id="myTabContent" class="tab-content ">
    <div class="tab-pane fade in active" id="home">
        <table class="table table-striped table-p">
            <% for(var i=0;i<10;i++){%>
            <tr>
                <%if(i<9){%>
                <td>0<%=i+1%></td>
                <%} else {%>
                <td><%=i+1%></td>
                <%}%>

                <td><%=locals.changxiao[i].name%></td>
                <td class="text-danger">￥<%=(locals.changxiao[i].price)/100%></td>
            </tr>

            <tr>
                <td></td>
                <td><span class="text-info"><%=locals.changxiao[i].author_id.name%></span></td>
                <td></td>
            </tr>
            <%}%>

        </table>
        <div class="text-right">
            <a href="/recommend">查看完整榜单&raquo;</a>
        </div>
    </div>
    <div class="tab-pane fade" id="ios">
        <table class="table table-striped">
            <% for(var i=0;i<10;i++){%>
            <tr>
                <%if(i<9){%>
                <td>0<%=i+1%></td>
                <%} else {%>
                <td><%=i+1%></td>
                <%}%>
                <td><%=locals.xinshu[i].name%></td>
                <td class="text-danger">￥<%=(locals.xinshu[i].price)/100%></td>
            </tr>
            <tr>
                <td></td>
                <td class="text-info"><%=locals.xinshu[i].author_id.name%></td>
                <td></td>
            </tr>
            <%}%>
        </table>
        <div class="text-right">
            <a href="/newBooks">查看完整榜单&raquo;</a>
        </div>
    </div>
</div>
</div>